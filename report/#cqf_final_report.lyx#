\begin_layout Subsection
Convergence and stability
\end_layout

\begin_layout Standard
Comparison between Pseudo-random and Quasi random numbers (Sobol, Niederreiter,
 Faure).
\end_layout

\begin_layout Standard
===> Niederreiter not applicable because it's limited to 318 dimension (in
 nag lib)
\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout
\begin_inset Graphics
	filename plot_cap_convergence_diagram.png
	scale 40

\end_inset


\end_layout

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Convergence diagram for a cap price
\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

